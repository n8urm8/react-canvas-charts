{
  "name": "react-charting-exploration",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "build:charts": "pnpm --filter react-canvas-charts build",
    "build:charts:watch": "pnpm --filter react-canvas-charts build:dev",
    "build:demo": "pnpm --filter charts-demo build",
    "dev": "pnpm --filter charts-demo dev",
    "build:dev": "pnpm --filter react-canvas-charts build:dev",
    "lint": "pnpm -r lint",
    "preview": "pnpm --filter charts-demo preview",
    "publish:charts": "pnpm --filter react-canvas-charts publish --access public"
  }
}
