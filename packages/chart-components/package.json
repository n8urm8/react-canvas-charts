{
  "name": "react-canvas-charts",
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-canvas-charts.umd.cjs",
  "module": "./dist/react-canvas-charts.js",
  "types": "./src/index.ts",
  "style": "./dist/react-canvas-charts.css",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./style.css": "./dist/react-canvas-charts.css",
    "./components": {
      "types": "./src/components/Chart/components/index.ts",
      "default": "./src/components/Chart/components/index.ts"
    }
  },
  "scripts": {
    "dev": "vite --port 5174",
    "build": "tsc -b && vite build",
    "build:dev": "vite build --watch",
    "build:analyze": "set ANALYZE_VISUALIZE=true && vite build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "babel-plugin-react-compiler": "1.0.0",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "6.0.0-rc1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "prettier": "3.6.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-plugin-dts": "^4.5.4"
  },
  "author": {
    "name": "n8urm8"
  },
  "license": "MIT",
  "homepage": "https://react-canvas-charts.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n8urm8/react-canvas-charts.git"
  },
  "keywords": [
    "react",
    "canvas",
    "typescript",
    "chart",
    "timeseries",
    "data-visualization"
  ]
}
